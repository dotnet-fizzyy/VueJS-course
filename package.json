{
  "name": "vuejs-course",
  "version": "0.0.1",
  "private": true,
  "description": "VueJS learning program",
  "author": {
    "name": "Dzmitry Yaniuk"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit --watch --useStderr --runInBand",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "storybook:build": "vue-cli-service storybook:build -c config/storybook",
    "storybook:serve": "vue-cli-service storybook:serve -p 6006 -c config/storybook",
    "ui": "vue ui",
    "upgrage": "vue upgrade --next"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11",
    "vue-lazyload": "^1.3.3",
    "vue-resource": "^1.5.3",
    "vuex": "3.6.2"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^6.0.26",
    "@storybook/addon-links": "^6.0.26",
    "@storybook/builder-webpack5": "^6.4.3",
    "@storybook/manager-webpack5": "^6.4.3",
    "@storybook/vue": "^6.0.26",
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/cli": "4.5.14",
    "@vue/cli-plugin-babel": "~5.0.4",
    "@vue/cli-plugin-e2e-cypress": "~4.5.14",
    "@vue/cli-plugin-eslint": "~5.0.4",
    "@vue/cli-plugin-typescript": "~5.0.4",
    "@vue/cli-plugin-unit-jest": "~5.0.4",
    "@vue/cli-service": "~5.0.4",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^1.0.3",
    "@vue/vue2-jest": "^27.0.0-alpha.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-storybook": "^0.5.8",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "image-webpack-loader": "^8.1.0",
    "jest": "^27.1.0",
    "jest-serializer-vue": "^2.0.2",
    "prettier": "^2.4.1",
    "sass": "^1.50.0",
    "sass-loader": "^12.6.0",
    "ts-jest": "^27.0.4",
    "typescript": "~4.5.5",
    "vue-cli-plugin-storybook": "~3.0.0-rc.0",
    "vue-template-compiler": "^2.6.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest/presets/typescript-and-babel",
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/(unit|integration)/**/*.spec.[jt]s?(x)"
    ],
    "setupFiles": ["<rootDir>/.jest/setupEnvVariables.js"]
  },
  "bugs": {
    "url": "https://github.com/dotnet-fizzyy/VueJS-course/issues"
  },
  "homepage": "https://github.com/dotnet-fizzyy/VueJS-course#readme",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dotnet-fizzyy/VueJS-course.git"
  }
}
